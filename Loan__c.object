    <webLinks>
        <fullName>Create_Loan_Schedules</fullName>
        <availability>online</availability>
        <description>This will execute code to create Loan Schedules.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Create Loan Schedules</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/soap/ajax/45.0/connection.js&apos;)}

var Status = &quot;{!Loan__c.Loan_Status__c}&quot;;
var ConStat = &quot;{!Loan__c.Rep_Status__c}&quot;;
var userId = &quot;{!$User.Id}&quot;;
var result = sforce.connection.query( 
              &quot;SELECT Id &quot; + 
              &quot;FROM PermissionSetAssignment &quot; + 
              &quot;WHERE PermissionSetId = &apos;0PS4A000000Drdc&apos; &quot; + 
              &quot;AND AssigneeId =&apos;{!$User.Id}&apos;&quot;
               ); 
var psAssignment = result.getArray(&quot;records&quot;); 

{!REQUIRESCRIPT(&apos;/js/functions.js&apos;)} 
{!REQUIRESCRIPT(&apos;/resource/jQueryForPopup/jQuery/jquery-1.8.2.min.js&apos;)} 
{!REQUIRESCRIPT(&apos;/resource/jQueryForPopup/jQuery/ui/jquery-ui-1.9.1.custom.min.js&apos;)}

{!REQUIRESCRIPT(&apos;/resource/jQueryForPopup/jQuery/postmessage/jquery.ba-postmessage.js&apos;)} 
{!REQUIRESCRIPT(&apos;/resource/jQueryForPopup/jQuery/bbq/jquery.ba-bbq.min.js&apos;)} 
requireCssFile(&apos;/resource/jQueryForPopup/jQuery/ui/css/ui-lightness/jquery-ui-1.9.1.custom.min.css&apos;); 
function requireCssFile(filename) 
{ 
var fileref = document.createElement(&apos;link&apos;);
fileref.setAttribute(&apos;rel&apos;, &apos;stylesheet&apos;);
fileref.setAttribute(&apos;type&apos;, &apos;text/css&apos;);
fileref.setAttribute(&apos;href&apos;, filename);
document.getElementsByTagName(&apos;head&apos;)[0].appendChild(fileref);
} 
var j$ = jQuery.noConflict();
var recId = window.location.href.substring(window.location.href.lastIndexOf(&apos;/&apos;) + 1);
var iframe_url = &apos;/apex/LoanForgiveness_Schedule_PopUp?id=&apos; + recId;
var child_domain = iframe_url.substring(0, iframe_url.indexOf(&apos;/&apos;, 9));
var parent_domain = window.location.protocol + &apos;//&apos; + window.location.host;
var j$modalDialog = j$(&apos;&lt;div id=&quot;opppopup&quot;&gt;&lt;/div&gt;&apos;)
.html(&apos;&lt;iframe id=&quot;iframeContentId&quot; src=&quot;&apos; + iframe_url + &apos;&quot; frameborder=&quot;0&quot; height=&quot;100%&quot; width=&quot;100%&quot; marginheight=&quot;0&quot; marginwidth=&quot;0&quot; scrolling=&quot;no&quot; /&gt;&apos;) 
.dialog({ 
autoOpen:false, 
title:&apos;LOAN AMORTIZATION &apos;, 
resizable:true, 
width:1000, 
height:540, 
autoResize:true, 
modal: true, 
draggable: true 
});
j$(&quot;.ui-dialog-titlebar&quot;).css({&quot;background&quot;: &quot;#AE5B2E&quot;, &quot;border&quot;: &quot;none&quot;}); 

if(Status != &apos;Open&apos;){ 
    alert(&apos;Loan Schedules can only be created when Loan Status is Open&apos;); 
}
else if(psAssignment.length == 0 &amp;&amp; &quot;{!$Profile.Name}&quot; != &apos;System Administrator&apos;){ 
alert(&apos;You do not have permissions to generate Loan Schedules&apos;); 
window.location.reload(); 
} 
else if(ConStat != &apos;Active&apos;){ 
    alert(&apos;Rep related to this Loan is Not Active to generate Schedules&apos;); 
    window.location.reload(); 
}
else{ 
    j$modalDialog.dialog(&apos;open&apos;);
}</url>
