    <validationRules>
        <fullName>Interest_Forgiven_Currency_Validation</fullName>
        <active>true</active>
        <description>Interest_Forgiven_Currency_Validation to be greater than $0 when not blank.</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Interest_Forgiven__c)),
Interest_Forgiven__c&lt; 1
)</errorConditionFormula>
        <errorDisplayField>Interest_Forgiven__c</errorDisplayField>
        <errorMessage>Interest Forgiven must always be greater than $0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Loan_Amount_Currency_Validation</fullName>
        <active>true</active>
        <description>Loan_Amount_currency_validation</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Original_Loan_Amount__c)),
Original_Loan_Amount__c &lt; 1
)</errorConditionFormula>
        <errorDisplayField>Original_Loan_Amount__c</errorDisplayField>
        <errorMessage>Loan Amount must always be greater than $0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Loan_Interest_must_be_greater_than_0</fullName>
        <active>true</active>
        <description>Loan Interest Rate must be greater than 0%</description>
        <errorConditionFormula>Loan_Interest_Rate__c   &lt;= 0</errorConditionFormula>
        <errorDisplayField>Loan_Interest_Rate__c</errorDisplayField>
        <errorMessage>Loan Interest Rate must be greater than 0%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Populate_Loan_Termed_Date</fullName>
        <active>true</active>
        <description>When Termed is selected in Status, Rep termed Date needs to be populated.</description>
        <errorConditionFormula>AND(ISPICKVAL(Loan_Status__c,&apos;Termed&apos;),
ISNULL(Rep_Termed_Date__c))</errorConditionFormula>
        <errorMessage>Please fill in Rep Termed Date when Terminating the Loan.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Principal_Forgiven_Currency_Validation</fullName>
        <active>true</active>
        <description>Principal_Forgiven_Currency_Validation to be greater than $0 when not blank.</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Principal_Forgiven__c)),
Principal_Forgiven__c &lt; 1
)</errorConditionFormula>
        <errorDisplayField>Principal_Forgiven__c</errorDisplayField>
        <errorMessage>Principal Forgiven must always be greater than $0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_edits_post_Schedule_creation</fullName>
        <active>true</active>
        <description>Restrict edits to fields on the Loan Object once Loan Schedules have been created.</description>
        <errorConditionFormula>AND(
Loan_Schedules_Created__c,
OR(ISCHANGED(Rep_Name__c),
ISCHANGED(Netsuite_Vendor_Id__c),
ISCHANGED(Original_Loan_Amount__c),
ISCHANGED(Loan_Interest_Rate__c),
ISCHANGED(Loan_Effective_Date__c),
ISCHANGED(Loan_Term__c),
ISCHANGED(Loan_Type__c)
)
)</errorConditionFormula>
        <errorMessage>No more edits permitted since Schedules have been generated. Please contact System Administrator</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Termed_Total_Currency_Validation</fullName>
        <active>true</active>
        <description>Termed_Total_Currency_Validation to be greater than $0 when not blank.</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Termed_Total_Balance_Due__c)),
Termed_Total_Balance_Due__c&lt; 1
)</errorConditionFormula>
        <errorDisplayField>Termed_Total_Balance_Due__c</errorDisplayField>
        <errorMessage>Termed Total Balance Due must always be greater than $0</errorMessage>
    </validationRules>
