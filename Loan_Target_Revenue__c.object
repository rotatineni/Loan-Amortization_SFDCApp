    <validationRules>
        <fullName>Target_Revenue_Currency_Validation</fullName>
        <active>true</active>
        <description>Target_Revenue_Currency_Validation</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Target_Revenue__c)),
Target_Revenue__c &lt; 1
)</errorConditionFormula>
        <errorDisplayField>Target_Revenue__c</errorDisplayField>
        <errorMessage>Target Revenue must be greater than $0</errorMessage>
    </validationRules>
