        <precision>14</precision>
        <required>true</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Target_Year__c</fullName>
        <description>This field is used to track the Year in date Format for reporting. Update this to default value for a year based on &apos;Target for Year&apos; field.</description>
        <externalId>false</externalId>
        <formula>DATE(VALUE(Target_for_Year__c),01,01)</formula>
        <label>Target Year</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Target_for_Year__c</fullName>
        <description>This field is updated by the WF Rule when value present in &apos;Target for Year of(Sys)&apos; field else User can input a year which will stay on the record.</description>
        <externalId>false</externalId>
        <label>Target for Year</label>
        <length>4</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
